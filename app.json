{
  "expo": {
    "name": "StoryCraft",
    "slug": "StoryCraft",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appicon.png",
    "scheme": "storycraft",

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appicon.png",
        "backgroundColor": "#ffffff",
        "monochromeImage": "./assets/images/appicon.png"
      },
      "package": "com.storycraft.android2"
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/appicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/appicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "gradleProperties": {
              "org.gradle.jvmargs": "-Xmx2048m -XX:MaxMetaspaceSize=512m",
              "android.useAndroidX": "true",
              "android.enableJetifier": "true",
              "android.enableR8.fullMode": "false",
              "android.enableDexingArtifactTransform.desugaring": "false"
            }
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "83772ac7-14a5-4efe-a2dd-8cade10bb754"
      }
    },
    "owner": "eazynick"
  },
  "metro": {
    "maxWorkers": 2,
    "resetCache": true
  }
}
